
1include <iostream>
1include<ctime>
using namespace std;

int main()

{
	const int ROW = 2;
	const int COL = 4;

	int arr[ROW][COL]{};

	for (int i = 0; i < ROW; i++)
	{
		for (int j = 0; j < COL; j++)
		{
			arr[i][j] = rand() % 100;
		}
	}
	 
	for (int i = 0; i < ROW; i++)
	{
		for (int j = 0; j < COL; j++)
		{
			cout << arr[i][j] << "\t";
		}
		cout << endl;
	}
	
	int array[8]{};
		
	for (int i = 0; i <8 ; i++)
	{
		array[8] = arr[ROW][COL];

	}
		
	cout << array[8] << endl;

}
